                " WHERE (r_serviciousuario.fkServicio = 8)" & _
                   " AND (r_serviciousuario.fechaInicio Between #" & Format(fInicio, "mm/dd/yyyy") & "# And #" & Format(fFin, "mm/dd/yyyy") & "#)" & _
                " GROUP BY r_serviciousuario.fkPersona"

    Set rs = New ADODB.Recordset
