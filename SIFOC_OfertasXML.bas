Private Function getSNE_COLRazonSocialEmpresa(idOferta As Long) As String
    Dim idOrganizacion As Long
    idOrganizacion = Nz(DLookup("fkOrganizacion", "t_oferta", "[id]=" & idOferta), 0)
    
